{
  "name": "ambitious-react-apps",
  "version": "1.0.0",
  "description": "How Ember developers can build ambitious ArcGIS apps with React",
  "main": "deck.mdx",
  "scripts": {
    "start": "mdx-deck deck.mdx",
    "build": "mdx-deck build deck.mdx",
    "export": "mdx-deck pdf --out-file=ambitious-react-apps.pdf deck.mdx",
    "predeploy": "npm run export && rm -rf dist/TEMP",
    "deploy": "gh-pages -d dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "React",
    "ArcGIS",
    "Ember"
  ],
  "author": "Tom Wayson <tom@tomwayson.com> (https://tomwayson.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomwayson/ambitious-react-apps.git"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "mdx-deck": "^1.8.2"
  },
  "dependencies": {
    "mdx-deck-code-surfer": "^0.5.5",
    "raw-loader": "^1.0.0"
  }
}
